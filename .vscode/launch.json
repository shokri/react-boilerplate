{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Reactjs: debug server-side",
      "type": "node-terminal",
      "request": "launch",
      "command": "yarn start"
    },
    {
      "name": "Reactjs: debug client-side",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:3000"
    }
  ],
  "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"]
}
